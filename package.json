		"vue-electron": "^1.0.6",
		"vue-property-decorator": "^8.4.0",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1",
		"vuex-electron": "^1.0.0"
	},
	"devDependencies": {
		"ajv": "^6.5.0",
